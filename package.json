{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run barrels && tsc --project tsconfig.compile.json && resolve-tspaths --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
